{
  "name": "mirai-foxes",
  "version": "1.5.16",
  "description": "一个根据 Mirai-js 代码重写，运行在 Node.js 下的 Typescript QQ 机器人开发框架",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:type": "tsc --noEmit -p ./tsconfig.json",
    "fix": "eslint ./src --fix",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurDevsCN/mirai-foxes.git"
  },
  "author": "FurDevsCN",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/FurDevsCN/mirai-foxes/issues"
  },
  "homepage": "https://github.com/FurDevsCN/mirai-foxes#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "typescript": "^5.1.6",
    "tsup": "^6.5.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "form-data": "^4.0.0",
    "ws": "^8.8.1"
  }
}
